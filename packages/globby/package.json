{
  "name": "@cjsa/globby",
  "version": "13.1.1",
  "description": "User-friendly glob matching",
  "license": "MIT",
  "repository": "sindresorhus/globby",
  "funding": "https://github.com/sponsors/sindresorhus",
  "author": {
    "email": "sindresorhus@gmail.com",
    "name": "Sindre Sorhus",
    "url": "https://sindresorhus.com"
  },
  "type": "commonjs",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "bench": "npm update @globby/main-branch glob-stream fast-glob && node bench.js",
    "test": "pnpm jest",
    "tocjs:patch-package-json": "pnpm patch-package-json globby",
    "prepare": "node build-cjs.js",
    "postpack": "pnpm jest"
  },
  "files": [
    "dist-cjs/*"
  ],
  "keywords": [
    "all",
    "array",
    "directories",
    "expand",
    "files",
    "filesystem",
    "filter",
    "find",
    "fnmatch",
    "folders",
    "fs",
    "glob",
    "globbing",
    "globs",
    "gulpfriendly",
    "match",
    "matcher",
    "minimatch",
    "multi",
    "multiple",
    "paths",
    "pattern",
    "patterns",
    "traverse",
    "util",
    "utility",
    "wildcard",
    "wildcards",
    "promise",
    "gitignore",
    "git"
  ],
  "dependencies": {
    "dir-glob": "^3.0.1",
    "fast-glob": "^3.2.11",
    "ignore": "^5.2.0",
    "merge2": "^1.4.1",
    "slash": "npm:@cjsa/slash@^4.0.0"
  },
  "devDependencies": {
    "@globby/main-branch": "sindresorhus/globby#main",
    "@types/node": "^17.0.10",
    "ava": "^4.0.1",
    "benchmark": "2.1.4",
    "get-stream": "^6.0.1",
    "glob-stream": "^7.0.0",
    "rimraf": "^3.0.2",
    "tsd": "^0.19.1",
    "typescript": "^4.5.5",
    "xo": "^0.47.0"
  },
  "xo": {
    "ignores": [
      "fixtures"
    ]
  },
  "ava": {
    "files": [
      "!tests/utilities.js"
    ],
    "workerThreads": false
  },
  "main": "./dist-cjs/index.cjs",
  "types": "./dist-cjs/index.d.ts"
}
